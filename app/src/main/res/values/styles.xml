<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/customtoolbar</item>
        <item name="colorPrimaryDark">@color/colorbutton</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="seekbars" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">@color/colorIcon</item>
        <item name="android:progressTint">#ff00</item>
        <item name="colorControlActivated">@color/customtoolbar</item>
    </style>

</resources>
